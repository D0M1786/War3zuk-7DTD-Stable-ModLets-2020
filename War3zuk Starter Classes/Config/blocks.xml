	<property name="CreativeMode" value="Dev"/>
	<property name="MaxDamage" value="250"/>
	<property name="Model" value="#@modfolder:Resources/Class Builder.unity3d?Class Bruiser VM.Prefab"/>
	<property name="TraderID" value="16"/>
	<property name="Stacknumber" value="1"/>
	<property name="EconomicValue" value="0"/>
	<drop event="Destroy" name="Class Bruiser VM" count="1"/>
	<property name="DescriptionKey" value="ClassVendingMachineDesc" />
